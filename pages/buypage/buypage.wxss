.flow-items-selected {
  /* border-bottom: 8rpx solid #2daf73; */
  box-sizing: border-box;
  color: #2daf73;
}

/**
去除横向滚动条
*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.item-icon {
  font-size: 44rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #0a0a0a;
}

.first-item-text {
  font-size: 18rpx;
  height: 24rpx;
  line-height: 24rpx;
  color: red;
}

.item-dec {
  font-size: 20rpx;
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 10rpx;
  color: #6c6c6c;
}

.input {
  font-size: 20rpx;
  margin-left: 25rpx;
  max-lines: 4;
  color: black;
}

.input-placeholder-class {
  font-size: 20rpx;
  color: #6c6c6c;
}

.placeholder {
  margin: 10rpx;
  height: 48rpx;
  width: auto;
  border-radius: 30px;
  background: #e8e8e8;
}

.placeholder-text {
  font-size: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}

.selected-color {
  color: #2daf73;
}

.selected-title-color {
  color: orange;
}

.selected-bottom-color {
  color: black;
}

