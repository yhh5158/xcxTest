.scroll-box .coupon-scroll-view_x {
  width: 100%;
  height: 140rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  white-space: nowrap;
  overflow: hidden;
  box-sizing: border-box;
}

.divider {
  display: block;
  height: 2rpx;
  /* width: 100%; *//* margin: 10rpx 30rpx; */
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  clear: both;
  box-sizing: border-box;
  border-top: 2rpx solid #e8e8e8;
}

.buytitle {
  font-size: 24rpx;
  height: 30rpx;
  line-height: 30rpx;
  margin-left: 25rpx;
  margin-bottom: 30rpx;
  color: #0a0a0a;
}

.flow-items {
  width: 166rpx;
  height: 128rpx;
  /* border-bottom: 8rpx solid #f7f7f7; */
  box-sizing: border-box;
  display: inline-block;
  /*text-align: center;*/
  position: relative;
}

.flow-item {
  width: 110rpx;
  height: 102rpx;
  position: absolute;
  /*border: #0C0C0C 1rpx solid;*/
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  text-align: center;
}

